    "next": "11.1.0",
    "next-connect": "^0.10.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.18",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "prisma": "^2.29.1",
    "typescript": "4.3.5"
