{
  "name": "next-ecommerce-prisma",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:db:push": "dotenv -e .env.local -- npx prisma db push",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "prisma:generate:client": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate:init": "dotenv -e .env.local -- npx prisma migrate dev --name init",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "prisma:seed": "dotenv -e .env.local -- npm run ts-node prisma/seed.ts --preview-feature",
    "prisma:introspect": "dotenv -e .env.local -- npx prisma introspect"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/data-grid": "^4.0.0-alpha.37",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mui/x-data-grid-generator": "^4.0.0",
    "@next-auth/prisma-adapter": "^0.5.4",
    "@prisma/client": "^2.30.3",
    "@sendgrid/mail": "^7.4.6",
    "@supabase/supabase-js": "^1.22.5",
    "@types/multer": "^1.4.7",
    "@types/react-select": "^4.0.17",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "express": "^4.17.1",
    "multer": "^1.4.3",
    "next": "11.1.2",
    "next-auth": "^3.28.0",
    "next-connect": "^0.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-select": "^4.3.1",
    "superjson": "^1.7.5"
  },
  "devDependencies": {
    "@types/react": "17.0.18",
    "babel-plugin-superjson-next": "^0.3.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "prisma": "^2.30.3",
    "ts-node": "^10.2.1",
    "typescript": "4.3.5"
  }
